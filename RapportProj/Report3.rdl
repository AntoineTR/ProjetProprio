  <DataSets>
    <DataSet Name="DataSet3">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT        DATENAME(month, DateDebutAnnonce) AS LeMois, COUNT(DateDebutAnnonce) AS nb, YEAR(GETDATE()) AS Year
FROM            Annonce
WHERE        (DateFinAnnonce IS NULL) AND (YEAR(GETDATE()) = YEAR(DateDebutAnnonce))
GROUP BY DATENAME(month, DateDebutAnnonce)
ORDER BY LeMois</CommandText>
      </Query>
