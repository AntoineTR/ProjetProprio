        <Field Name="Year">
          <DataField>Year</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT        DATENAME(month, DateDebutAnnonce) AS LeMois, COUNT(DateDebutAnnonce) AS nb, YEAR(GETDATE()) AS Year
FROM            Annonce
WHERE        (DateFinAnnonce IS NULL) AND (YEAR(GETDATE()) = YEAR(DateDebutAnnonce))
GROUP BY DATENAME(month, DateDebutAnnonce)</CommandText>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
