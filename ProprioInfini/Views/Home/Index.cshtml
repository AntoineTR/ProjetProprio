<script type="text/javascript">
    window.onload(initialize());
    function initialize() {
        var mapOptions = {
            center: new google.maps.LatLng(45.4959944, -73.5693213),
            zoom: 8,
            mapTypeId: google.maps.MapTypeId.ROADMAP
        };
        var map = new google.maps.Map(document.getElementById("map"), mapOptions);
        addPoint(map, new google.maps.LatLng(45.539121, -73.492355));
    }

    function addPoint(map, point) {
        geocoder = new google.maps.Geocoder();
        geocoder.geocode({ 'address': '745 chemin chambly Longueuil' }, function (results, status) {
           if (status == google.maps.GeocoderStatus.OK)
            {
                var position = results[0].geometry.location;
            }
            else
            {
                alert('Geocode was not successful for the following reason: ' + status);
            }
        });

        marker = new google.maps.Marker({
            position: point,
            map: map
        });

    }
</script>
