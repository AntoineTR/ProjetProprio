@model ProprioInfini.Annonce

@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

@using (Html.BeginForm()) {
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <fieldset>
        <legend>Annonce</legend>

        @Html.HiddenFor(model => model.Id)

        <div class="editor-label">
            @Html.LabelFor(model => model.ProprietaireId, "Proprietaire")
        </div>
        <div class="editor-field">
            @Html.DropDownList("ProprietaireId", String.Empty)
            @Html.ValidationMessageFor(model => model.ProprietaireId)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.DateDebutAnnonce)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.DateDebutAnnonce)
            @Html.ValidationMessageFor(model => model.DateDebutAnnonce)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.DateFinAnnonce)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.DateFinAnnonce)
            @Html.ValidationMessageFor(model => model.DateFinAnnonce)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Prix)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Prix)
            @Html.ValidationMessageFor(model => model.Prix)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.BatimentId, "Batiment")
        </div>
        <div class="editor-field">
            @Html.DropDownList("BatimentId", String.Empty)
            @Html.ValidationMessageFor(model => model.BatimentId)
        </div>

        <div class="editor-label">
            @Html.LabelFor(model => model.Titre)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.Titre)
            @Html.ValidationMessageFor(model => model.Titre)
        </div>

        <p>
            <input type="submit" value="Save" />
        </p>
    </fieldset>
}

<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}
